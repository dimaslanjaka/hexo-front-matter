{
  "name": "hexo-front-matter",
  "version": "4.2.1",
  "description": "Front-matter parser.",
  "main": "dist/front_matter.js",
  "scripts": {
    "prepublish ": "npm run clean && npm run build",
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "eslint": "eslint .",
    "test": "mocha test/index.js --require ts-node/register",
    "test-cov": "c8 --reporter=lcovonly npm run test",
    "enable-husky": "husky install",
    "disable-husky": "git config --unset core.hooksPath"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/front_matter.d.ts",
  "repository": "hexojs/hexo-front-matter",
  "keywords": [
    "front-matter",
    "front matter",
    "yaml",
    "yml",
    "hexo",
    "json"
  ],
  "author": "Tommy Chen <tommy351@gmail.com> (http://zespia.tw)",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.7",
    "c8": "^9.1.0",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "eslint-config-hexo": "^5.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  }
}
